{
  "name": "lagomstronk",
  "version": "1.0.0",
  "description": "Track your gym workouts, progress, and personal records",
  "main": "index.html",
  "scripts": {
    "start": "npx serve .",
    "cap:init": "npx cap init LagomStronk se.lagomstronk.app --web-dir .",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "build:android": "npx cap sync && npx cap open android"
  },
  "keywords": [
    "gym",
    "fitness",
    "workout",
    "tracker",
    "pwa"
  ],
  "author": "Jeppe Morris",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "serve": "^14.0.0"
  }
}
